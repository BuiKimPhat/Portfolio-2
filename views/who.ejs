    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/3.0.2/fullpage.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/3.0.2/vendors/easings.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/3.0.2/fullpage.css" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/3.0.2/fullpage.extensions.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/barba.js/1.0.0/barba.min.js" type="text/javascript"></script>
    <script src="/style/whoAni.js"></script>
    <script src="/style/transition.js"></script>
    <link rel="stylesheet" type="text/css" href="/style/navbar.css" />
    <title>EO Team - Who</title>
  </head>
  <body>
    <% include partials/nav.ejs %>
      <div id="barba-wrapper">
        <div class="barba-container">
          <link rel="stylesheet" type="text/css" href="/style/who.css" />

            <div id="fullpage">
              <div class="section" data-anchor='w1'>
                <div class="mems row">
                  <div class="meo col-sm-4">
                    <a href="#w2"><img data-toggle="popover" title="Meo" data-placement="right" data-trigger="hover" data-content="Designer" src="/style/images/meo.jpg" class="img-fluid" /></a>
                  </div>
                  <div class="neo col-sm-4">
                    <a href="#w3"><img data-toggle="popover" title="Neo" data-placement="left" data-trigger="hover" data-content="Leader" src="/style/images/neo.jpg" class="img-fluid" /></a>
                  </div>
                  <div class="leo col-sm-4">
                    <a href="#w4"><img data-toggle="popover" title="Leo" data-placement="left" data-trigger="hover" data-content="Front-end" src="/style/images/leo.jpg" class="img-fluid" /></a>
                  </div>
                </div>
              </div>
              <div class="section" data-anchor='w2'>
                <div class="slide">
                  <div class="row">
                    <img data-toggle="popover" title="Meo" data-placement="right" data-trigger="hover" data-content="Designer" src="/style/images/meo.jpg" class="col-sm-5"/>
                    <div class="meoi info text-center col-sm-7">
                      <h3>THIS IS MEO</h3>
                      <h4>Full name: Nguyễn Lê Anh Quân</h4>
                      <h4>Designer</h4>
                      <h4>Meo</h4>
                      <h4>Meme Lord</h4>
                      <h4>Dabber</h4>
                    </div>
                  </div>
                </div>
                <div class="slide">
                </div>
              </div>
              <div class="section" data-anchor='w3'>
                <div class="slide">
                  <div class="row">
                    <div class="neoi info text-center col-sm-7">
                      <h3>THIS IS NEO</h3>
                      <h4>Full name: Phạm Quang Mẫn</h4>
                      <h4>Gank team</h4>
                      <h4>Neo</h4>
                      <h4>Metal-head</h4>
                      <h4>Movie nerd</h4>
                    </div>
                    <img data-toggle="popover" title="Neo" data-placement="left" data-trigger="hover" data-content="Leader" src="/style/images/neo.jpg" class="col-sm-5"/>
                  </div>
                </div>
                <div class="slide">

                </div>
              </div>
              <div class="section" data-anchor='w4'>
                <div class="slide">
                  <div class="row">
                    <img data-toggle="popover" title="Leo" data-placement="right" data-trigger="hover" data-content="Front-end" src="/style/images/leo.jpg" class="col-sm-5"/>
                    <div class="leoi info text-center col-sm-7">
                      <h3>THIS IS LEO</h3>
                      <h4>Full name: Bùi Kim Phát</h4>
                      <h4>Front-end</h4>
                      <h4>Leo</h4>
                      <h4>Nhận biết mọi anime chỉ từ màu tóc main</h4>
                      <h4>Thành thạo tiếng Gia Lai</h4>
                    </div>
                  </div>
                </div>
                <div class="slide">

                </div>
              </div>
              <script>
              $('#fullpage').fullpage(who);
              $.fn.fullpage.setAllowScrolling(true);
              Barba.Dispatcher.on("newPageReady",function(){
                fullpage_api.destroy('all');
              });
              Barba.Dispatcher.on("transitionCompleted",function(){
                var path = location.pathname;
                if (path == '/'){
                  $('#fullpage').fullpage(index);
                };
                if (path == '/who'){
                  $('#fullpage').fullpage(who);
                  whoani();
                };
              });

              </script>
            </div>
      </div>
    </div>
  </body>
</html>
